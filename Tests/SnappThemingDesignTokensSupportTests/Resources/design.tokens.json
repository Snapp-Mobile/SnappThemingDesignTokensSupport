{
    "base": {
        "color": {
            "red": {
                "$type": "color",
                "$value": "#FF0000"
            },
            "green": {
                "$type": "color",
                "$value": "#00FF00"
            },
            "blue": {
                "$type": "color",
                "$value": "#0000FF"
            },
            "black": {
                "$type": "color",
                "$value": "#000000"
            },
            "white": {
                "$type": "color",
                "$value": "#FFFFFF"
            }
        },
        "dimension": {
            "spacing10": {
                "$type": "dimension",
                "$value": {
                    "unit": "px",
                    "value": 10
                }
            },
            "spacing20": {
                "$type": "dimension",
                "$value": {
                    "unit": "px",
                    "value": 20
                }
            }
        }
    },
    "semantic": {
        "color": {
            "primary": { "$value": "{base.color.red}" },
            "secondary": { "$value": "{base.color.green}" },
            "tertiary": { "$value": "{base.color.blue}" },
            "text": {
                "primary": {
                    "light": { "$value": "{base.color.black}" },
                    "dark": { "$value": "{base.color.white}" }
                }
            }
        },
        "spacing": {
            "small": { "$value": "{base.dimension.spacing10}" },
            "medium": { "$value": "{base.dimension.spacing20}" }
        },
        "fontFamily": {
            "body": {
                "$type": "fontFamily",
                "$value": ["HelveticaNeue", "Helvetica"]
            }
        },
        "fontSize": {
            "body": {
                "$type": "dimension",
                "$value": {
                    "unit": "rem",
                    "value": 1
                }
            },
            "h1": {
                "$type": "dimension",
                "$value": {
                    "unit": "rem",
                    "value": 2
                }
            }
        },
        "typography": {
            "body": {
                "$type": "typography",
                "$value": {
                    "fontFamily": "{semantic.fontFamily.body}",
                    "fontSize": "{semantic.fontSize.body}",
                    "fontWeight": 400,
                    "letterSpacing": {
                        "value": 0.1,
                        "unit": "px"
                    },
                    "lineHeight": 1.2
                }
            },
            "h1": {
                "$type": "typography",
                "$value": {
                    "fontFamily": "{semantic.fontFamily.body}",
                    "fontSize": "{semantic.fontSize.h1}",
                    "fontWeight": 700,
                    "letterSpacing": {
                        "value": 0.1,
                        "unit": "px"
                    },
                    "lineHeight": 1.2
                }
            }
        },
        "gradient": {
            "fade-out": {
                "$type": "gradient",
                "$value": [
                    {
                        "color": "#000000",
                        "position": 0
                    },
                    {
                        "color": "#00000000",
                        "position": 1
                    }
                ]
            }
        }
    },
    "files": {
        "images": {
            "alien": {
                "$type": "file",
                "$value": "alien.svg"
            },
            "alien2": {
                "$type": "file",
                "$value": "alien.png"
            },
            "alien3": {
                "$type": "file",
                "$value": "alien.jpg"
            }
        },
        "animations": {
            "loading": {
                "$type": "file",
                "$value": "loading.lottie"
            }
        },
        "fonts": {
            "arialBlack": {
                "$type": "file",
                "$value": "ArialBlack.ttf"
            }
        }
    },
    "emptyGroup": {
        "unknownSubGroup": {
            "unknownToken": {}
        }
    },
    "unknownToken": {}
}
